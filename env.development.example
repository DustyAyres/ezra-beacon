# Development Environment Configuration
# Copy this to .env for local development without Azure AD

# Azure AD Configuration (can be dummy values in dev mode)
AZURE_CLIENT_ID=dummy-client-id
AZURE_TENANT_ID=dummy-tenant-id
AZURE_DOMAIN=dummy.onmicrosoft.com
AZURE_REDIRECT_URI=http://localhost:3000

# Development Mode - Bypasses authentication
# WARNING: Never use this in production!
BYPASS_AUTH=true
