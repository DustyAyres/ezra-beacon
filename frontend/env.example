# Frontend Environment Variables
# Copy this file to .env and update with your actual values

# Backend API URL
# For local development with backend running on port 5000
REACT_APP_API_URL=http://localhost:5000/api

# Azure AD Configuration
# Get these values from your Azure AD app registration
REACT_APP_AZURE_CLIENT_ID=your-azure-ad-client-id
# Set to 'common' for multi-tenant, or your specific tenant ID for single-tenant
REACT_APP_AZURE_TENANT_ID=your-azure-ad-tenant-id

# OAuth Redirect URI
# This must match what's configured in your Azure AD app registration
REACT_APP_AZURE_REDIRECT_URI=http://localhost:3000

# Development Authentication Bypass
# Set to 'true' to enable the "Continue as Developer" button
# WARNING: Never set this to 'true' in production!
REACT_APP_BYPASS_AUTH=true
